   GetAverageKarma(lObjIgnore=$, iOptionalRadiusDistance=$, oOptionalRadiusFromObj=$, bIncludePlayers=TRUE, bIncludeMonsters=TRUE, bIncludeNonInteractivePlayers=FALSE, bIncludeReflections=FALSE, bIncludeEvilTwin=FALSE, bIncludeServants=FALSE)
   "Checks players and monsters in the room and gives you an average karma."
   "Helpful to determine if you should be using Melancholy or Mirth (jala), or Qorbane / Shallybane in PVP."
   {
      local i, o, each_obj, iCount, iTotal, bIsInRadius, lObjPos, lState, iRange, bSkip, aMobAttributes;

      iCount = 0;
      iTotal = 0;

      if iOptionalRadiusDistance <> $ AND iOptionalRadiusDistance > 0 AND oOptionalRadiusFromObj <> $
      {
         lObjPos = Send(oOptionalRadiusFromObj, @GetPos);
      }

      foreach i in plActive
      {
         each_obj = First(i);

         bSkip = FALSE;

         if lObjIgnore <> $ AND IsList(lObjIgnore)
         {
            foreach o in lObjIgnore
            {
               if each_obj = o
               {
                  bSkip = TRUE;
                  //Debug("[karma] ignored",Send(each_obj,@GetName));
                  break;
               }
            }
         }

         if bSkip OR IsClass(each_obj, &DM)
         {
            continue;
         }

         if NOT IsClass(each_obj, &Player) AND NOT IsClass(each_obj, &Monster)
         {
            // We only count things that can return karma.
            continue;
         }

         if IsClass(each_obj, &Monster)
         {
            aMobAttributes = Send(each_obj,@GetAttributes);

            if (aMobAttributes & MOB_RECEIVE)
               OR (aMobAttributes & MOB_SELLER)
               OR (aMobAttributes & MOB_NOFIGHT)
            {
               // We only count real monsters.
               continue;
            }
         }
         
         if IsClass(each_obj,&Player) AND NOT bIncludePlayers
         {
            // Do not count player karma in room.
            //Debug("[karma] No count players",Send(each_obj,@GetName));
            continue;
         } 
         else if IsClass(each_obj,&Player) AND bIncludePlayers 
         {
            // Count player karma in room.
            if NOT ((bIncludeNonInteractivePlayers AND Send(each_obj,@IsInCannotInteractMode))
               OR (NOT bIncludeNonInteractivePlayers AND NOT Send(each_obj,@IsInCannotInteractMode)))
            {
               // Do not count non-interactive players.
               //Debug("[karma] No Count non-interactive player",Send(each_obj,@GetName));
               continue;
            }
         }

         if IsClass(each_obj,&Monster) AND NOT bIncludeMonsters
         {
            // Do not count monsters in room.
            //Debug("[karma] No Count Monster",Send(each_obj,@GetName));
            continue;
         }
         else if IsClass(each_obj,&Monster) AND bIncludeMonsters
         {
            if (IsClass(each_obj,&Servant) AND NOT bIncludeServants)
               OR (IsClass(each_obj,&Reflection) AND NOT bIncludeReflections)
               OR (IsClass(each_obj,&EvilTwin) AND NOT bIncludeEvilTwin)
            {
               // Do not count servants (or) reflections (or) evil twins in room.
               //Debug("[karma] Skipped Monster",GetClass(each_obj));
               continue;
            }
         }

         if iOptionalRadiusDistance <> $ AND iOptionalRadiusDistance > 0 AND oOptionalRadiusFromObj <> $
         {
            if lObjPos <> $ AND IsList(lObjPos)
            {
               iRange = Send(each_obj,@SquaredFineDistanceToLocation3D,
                  #row=Nth(lObjPos, 1),
                  #col=Nth(lObjPos, 2),
                  #fine_row=Nth(lObjPos, 3),
                  #fine_col=Nth(lObjPos, 4));
               
               if iRange <> $ AND iRange <> 0 AND iRange > (iOptionalRadiusDistance * iOptionalRadiusDistance)
               {
                  // Object is outside of our radius.
                  //Debug("[karma] outside radius",iRange,"<",(iOptionalRadiusDistance * iOptionalRadiusDistance),Send(oOptionalRadiusFromObj,@GetName));
                  continue;
               }
            }
            else
            {
               // We could not determine the position of the optional radius object.
               //Debug("[karma] Could not get pos from",Send(oOptionalRadiusFromObj,@GetName));
               continue;
            }
         }

         // All checks are good, count this player or monster.
         //Debug("[karma] Counted",Send(each_obj,@GetName));
         if Send(each_obj,@GetKarma) <> 0
         {
            iCount = iCount + 1;
            iTotal = iTotal + Send(each_obj,@GetKarma);
         }
      }

      if iCount > 0 AND iTotal <> 0
      {
         //Debug("[karma] Average",iTotal," / ",iCount," = ",(iTotal / iCount));
         return iTotal / iCount;
      }

      return 0;
   }

   EnchantPlayerStatusChanged(who=$)
   "Called when there's change in status of a player. Any enchantments they "
   "cast should re-check to see if they now affect any other players, and "
   "other enchantments should check if they now affect the player."
   {
      local i, state, oSpell, lObj, oUser;

      foreach i in plEnchantments
      {
         oSpell = Nth(i,2);
         state = Nth(i,3);

         // Only applies to spells that come with conditional effects,
         // e.g. winds lowers aim on players the caster can attack.
         if (state = $
            OR NOT Send(oSpell,@HasConditionalEffect))
         {
            continue;
         }

         if (Nth(state,2) = who)
         {
            // If 'who' cast the spell, check everyone.
            foreach lObj in plActive
            {
               oUser = First(lObj);
               if IsClass(oUser,&User)
               {
                  if (Send(oUser,@IsEnchanted,#what=oSpell))
                  {
                     if (NOT Send(oSpell,@CanEnchantUser,#who=oUser,#state=state))
                     {
                        Send(oSpell,@RemoveEnchantment,#who=oUser);
                     }
                  }
                  else if (Send(oSpell,@CanEnchantUser,#who=oUser,#state=state))
                  {
                     Send(oSpell,@StartEnchantment,#who=oUser,#state=state);
                  }
               }
            }
         }
         else if (Send(who,@IsEnchanted,#what=oSpell))
         {
            if (NOT Send(oSpell,@CanEnchantUser,#who=who,#state=state))
            {
               // Should no longer be affected by this spell.
               // e.g. deguilded, lost a faction shield.
               Send(oSpell,@RemoveEnchantment,#who=who);
            }
         }
         else if (Send(oSpell,@CanEnchantUser,#who=who,#state=state))
         {
            // Should be affected, e.g. turned outlaw.
            Send(oSpell,@StartEnchantment,#who=who,#state=state);
         }
      }

      // Radius enchant recalcs.
      foreach i in plActive
      {
         oUser = First(i);
         if (oUser = who)
         {
            Send(oUser,@NoMoveOnRadiusEnchCheckAll);
         }
         else if (IsClass(oUser,&NoMoveOn))
         {
            Send(oUser,@NoMoveOnRadiusEnchCheckOne,#who=who);
         }
      }

      return;
   }

