      foreach i in plActive
      {
         each_obj = First(i);   // optimized for room
         if IsClass(each_obj,&User) AND each_obj <> what
         {
            propagate;
         }
      }

      Send(self,@LastUserLeft,#what=what);

      propagate;
   }
   
