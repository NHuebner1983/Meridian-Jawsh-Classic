   oo_barbolt2_name = "bolt of cloth"
   oo_barbolt2_desc = "This is a bolt of fine cloth."

   oo_barbolt3_name = "bolt of cloth"
   oo_barbolt3_desc = "This is a bolt of fine cloth."

   oo_barbolt4_name = "bolt of cloth"
