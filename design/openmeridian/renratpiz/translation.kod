   oo_rockB_name = "rock"
   oo_rockB_desc = "A big rock."
   oo_rockC_name = "boulder"
   oo_rockC_desc = "A chunk of Kardde limestone."

   oo_goathead_name = "goat head"
   oo_goathead_desc = "This must be the grisly remains of some bizarre mystical ritual.  "
