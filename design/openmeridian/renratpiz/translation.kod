   oo_tree3_name = "motubanak tree"
   oo_tree3_desc = "The motubanak tree is said to be the only place that kriipa will sleep."

   oo_head2_name = "skull"
   oo_head2_desc = "A freakishly large skull sits atop a wooden post.  A warning, perhaps?"
        
